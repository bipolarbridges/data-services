{
    "compilerOptions": {
        "module": "CommonJS",
        "outDir": "./dist",
        "noImplicitAny": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "allowJs": true,
        "checkJs": true,
        "strictBindCallApply": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "skipLibCheck": true
    },
    "exclude": [
        "./node_modules",
        "./dist"
    ]

}